{
	"extends": "./.svelte-kit/tsconfig.json",
	"compilerOptions": {
		"allowJs": true,
		"checkJs": true,
		"esModuleInterop": true,
		"forceConsistentCasingInFileNames": true,
		"resolveJsonModule": true,
		"skipLibCheck": true,
		"sourceMap": true,
		"strict": true,
		"target": "es2020",
		"paths": {
			"$stores": [
				"src/stores"
			],
			"$components": [
				"src/components"
			],
			"$lib": [
				"src/lib"
			],
  		"$lib/*": [
				"src/lib/*"
			]
		}
	}
}
